{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno types",
      "cwd": "tests"
    },
    "flags:common": {
      "task": "deno types",
      "cwd": "tests",
      "types": {
        "quiet": true
      }
    }
  }
}
