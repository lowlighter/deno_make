{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno info"
    },
    "flags:lock_1": {
      "task": "deno info",
      "deno": {
        "info": {
          "lock": true
        }
      }
    },
    "flags:lock_2": {
      "task": "deno info",
      "deno": {
        "info": {
          "lock": false
        }
      }
    },
    "flags:lock_3": {
      "task": "deno info",
      "deno": {
        "info": {
          "lock": "deno.lock"
        }
      }
    }
  }
}
