{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno doc tests/mod.ts"
    },
    "flags:options": {
      "task": "deno doc tests/mod.ts",
      "deno": {
        "doc": {
          "private": true,
          "json": true
        }
      }
    }
  }
}
