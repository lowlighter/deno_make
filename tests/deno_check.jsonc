{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno check mod.ts",
      "cwd": "tests"
    },
    "flags:boolean_true": {
      "task": "deno check mod.ts",
      "cwd": "tests",
      "deno": {
        "check": {
          "all": true
        }
      }
    },
    "flags:boolean_false": {
      "task": "deno check mod.ts",
      "cwd": "tests",
      "deno": {
        "check": {
          "all": false
        }
      }
    }
  }
}
