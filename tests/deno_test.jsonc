{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno test mod_test.ts",
      "cwd": "tests"
    },
    "flags:boolean_true": {
      "task": "deno test mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "doc": true,
          "traceOps": true,
          "failFast": true,
          "shuffle": true,
          "parallel": true,
          "run": true,
          "allowNone": true
        }
      }
    },
    "flags:boolean_false": {
      "task": "deno test mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "doc": false,
          "traceOps": false,
          "failFast": false,
          "shuffle": false,
          "parallel": false,
          "run": false,
          "allowNone": false
        }
      }
    },
    "flags:number": {
      "task": "deno test mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "failFast": 1,
          "shuffle": 1
        }
      }
    },
    "flags:string": {
      "task": "deno test tests/mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "coverage": ".coverage",
          "reporter": "junit",
          "filter": "junit",
          "junitPath": "tests/.junit.xml"
        }
      }
    },
    "flags:array": {
      "task": "deno test tests/mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "ignore": ["*_test.ts"]
        }
      }
    },
    "flags:array_empty": {
      "task": "deno test tests/mod_test.ts",
      "cwd": "tests",
      "deno": {
        "test": {
          "ignore": []
        }
      }
    }
  }
}
