{
  "tasks": {
    "make": "deno run --allow-env --allow-read --allow-write=.deno-make.json --allow-run=deno ./mod.ts $0"
  },
  "+tasks": {
    "flags:none": {
      "task": "deno vendor mod.ts",
      "cwd": "tests"
    },
    "flags:output_1": {
      "task": "deno run mod.ts",
      "cwd": "tests",
      "deno": {
        "vendor": {
          "output": "vendor"
        }
      }
    },
    "flags:output_2": {
      "task": "deno run mod.ts",
      "cwd": "tests",
      "deno": {
        "vendor": {
          "output": {
            "path": "vendor",
            "force:": true
          }
        }
      }
    },
    "flags:output_3": {
      "task": "deno run mod.ts",
      "cwd": "tests",
      "deno": {
        "vendor": {
          "output": {
            "force:": false
          }
        }
      }
    },
    "flags:lock": {
      "task": "deno run mod.ts",
      "cwd": "tests",
      "deno": {
        "vendor": {
          "lock": "deno.lock"
        }
      }
    }
  }
}
